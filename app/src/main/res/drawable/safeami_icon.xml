<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size android:width="60dp" android:height="60dp"/>
            <solid android:color="#53B8F4" />
        </shape>
    </item>
    <item android:top="10dp" android:left="10dp" android:right="10dp" android:bottom="10dp">
        <shape android:shape="oval">
            <size android:height="2dp" android:width="2dp"/>
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
    <item android:top="10dp" android:left="10dp" android:right="10dp" android:bottom="35dp">
        <shape android:shape="oval">
            <size android:height="2dp" android:width="2dp"/>
            <solid android:color="#53B8F4" />
        </shape>
    </item>
    <item android:gravity="center" android:top="10dp" android:bottom="10dp" android:left="10dp" android:right="10dp">
        <shape android:shape="oval">
            <size android:height="10dp" android:width="10dp"/>
            <solid android:color="#000000" />
        </shape>
    </item>
</layer-list>